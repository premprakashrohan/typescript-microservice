{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [false],
        "quotemark": [true, "double"],
        "ordered-imports": false,
        "no-inferrable-types": false,
        "prefer-const": false,
        "eofline": false,
        "max-line-length": false,
        "member-ordering": false,
        "no-console": false,
        "indent": false
    }
}